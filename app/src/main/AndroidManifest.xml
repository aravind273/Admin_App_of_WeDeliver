<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adminapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/adminlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/adminlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminApp">
        <activity android:name=".PieChart"></activity>
        <activity android:name=".answer" />
        <activity android:name=".show_support_requests" />
        <!--
               Set to true if your app is Standalone, that is, it does not require the handheld
               app to run.
        -->
        <activity android:name=".rental" />
        <activity android:name=".laundry" />
        <activity android:name=".cart" />
        <activity android:name=".Item_successfully_added" />
        <activity android:name=".show_users" />
        <activity android:name=".show_orders" />
        <activity android:name=".add_items" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>